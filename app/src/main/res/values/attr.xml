<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SuperBannerView">
        <attr name="openSuperMode" format="boolean" />
        <attr name="sideAlpha" format="float" />
        <attr name="indicatorAlign" format="enum">
            <enum name="left" value="2" />
            <enum name="right" value="1" />
            <enum name="center" value="0" />
        </attr>
        <attr name="indicatorType" format="enum">
            <enum name="circle" value="0" />
            <enum name="number" value="1" />
        </attr>
    </declare-styleable>
</resources>